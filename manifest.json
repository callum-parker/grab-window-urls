{
	"manifest_version": 2,

	"name": "window urls saver",
	"description": "grab all urls in the current window as text, and load them back up again from said text.",
	"version": "0.0.1",

	"browser_action": {
		"default_icon": {
			"16": "images/icon/16.png",
			"32": "images/icon/32.png",
			"48": "images/icon/48.png",
			"128": "images/icon/128.png"
		},
		"default_popup": "popup.html"
	},
  
	"permissions": [
	"windows",
	"tabs"
	],
   
	"icons": {
		"16": "images/icon/16.png",
		"32": "images/icon/32.png",
		"48": "images/icon/48.png",
		"128": "images/icon/128.png"
	}
}